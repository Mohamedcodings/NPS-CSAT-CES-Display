<div id="upload-section">
  <h2>Upload Surveys</h2>
  <%= form_with url: surveys_path, local: true, method: :post, multipart: true do %>
    <div class="form-group">
      <%= file_field_tag :file %>
    </div>
    <div class="form-group">
      <%= submit_tag 'Upload' %>
    </div>
  <% end %>
</div>
